      "id": "call-openai-api",
      "name": "调用OpenAI API",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [460, 300]
    },
    {
      "parameters": {
        "jsCode": "const apiResult = $input.first().json;\n\n// 检查 API 错误\nif (apiResult.error) {\n  return [{ json: { \n    success: false, \n    error: apiResult.error.message || 'OpenAI API 调用失败',\n    code: apiResult.error.code\n  } }];\n}\n\n// 处理返回的 Projects 列表\nconst projects = apiResult.data || [];\n\nconst formattedProjects = projects.map(p => ({\n  id: p.id,\n  name: p.name,\n  status: p.status,\n  created_at: p.created_at ? new Date(p.created_at * 1000).toISOString() : null\n}));\n\nreturn [{ json: { \n  success: true,\n  projects: formattedProjects,\n  total: formattedProjects.length\n} }];"
      },
      "id": "process-result",
      "name": "处理结果",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
