    {
      "parameters": {
        "method": "GET",
        "url": "https://api.openai.com/v1/organization/projects",
        "sendQuery": true,
        "queryParameters": {
          "parameters": [
            { "name": "limit", "value": "100" },
            { "name": "include_archived", "value": "false" }
          ]
        },
